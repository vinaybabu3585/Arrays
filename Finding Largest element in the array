#include<stdio.h>
int main()
{
    int size;
    printf("Enter the size: ");
    scanf("%d",&size);
    
    int arr[size];
    printf("Enter the %d array elements: ");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int large=arr[0];
    printf("Sum of Array elements: ");
    for(int i=1;i<size;i++)
    {
        if(arr[i]>arr[i+1])
        large=arr[i];
    }
    printf("%d ",large);
   
    return 0;
}
