#include<stdio.h>
int main()
{
    int size;
    printf("Enter the size: ");
    scanf("%d",&size);
    
    int arr[size];
    printf("Enter the %d array elements: ");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int sum=0;
    printf("Sum of Array elements: ");
    for(int i=0;i<size;i++)
    {
        sum=sum+arr[i];
    }
    printf("%d",sum);
    return 0;
}
