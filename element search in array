// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
   int size;
   printf("Enter the size: ");
   scanf("%d",&size);
   
   int arr[size];
   printf("Enter the %d elemens: ");
   for(int i=0;i<size;i++)
   {
       scanf("%d",&arr[i]);
   }
   int ele,flag=0;
   printf("Enter the element to search: ");
   scanf("%d",&ele);
   for(int i=0;i<size;i++)
   {
       if(arr[i]==ele)
       {
           flag=1;
           break;
       }
   }
   flag?printf("Avaiable"):printf("Not available");

    return 0;
}
